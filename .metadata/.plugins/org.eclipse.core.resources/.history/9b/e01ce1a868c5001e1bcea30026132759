package model;

public class Author {
	int id;
	String fNameString;

	
	public Author() {
	}

	public Author(int id, String fNameString) {
		this.id = id;
		this.fNameString = fNameString;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getfNameString() {
		return fNameString;
	}

	public void setfNameString(String fNameString) {
		this.fNameString = fNameString;
	}

	@Override
	public String toString() {
		return "Author [id=" + id + ", fNameString=" + fNameString + "]";
	}
	
}
